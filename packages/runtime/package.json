{
  "name": "lumix-js",
  "version": "0.1.4",
  "description": "The modular runtime for LumixJS",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": { "types": "./dist/index.d.ts", "import": "./dist/index.js", "require": "./dist/index.js" },
    "./vinxi": { "types": "./dist/vinxi.d.ts", "import": "./dist/vinxi.js", "require": "./dist/vinxi.js" }
  },
  "files": [
    "dist/",
    "bin/",
    "templates/"
  ],
  "bin": {
    "lumix": "./bin/lumix.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "keywords": [
    "lumix-js",
    "runtime",
    "signals",
    "framework",
    "framework-cli"
  ],
  "author": "ZMDev",
  "license": "MIT",
  "devDependencies": {
    "happy-dom": "^20.6.1",
    "typescript": "^5.0.0",
    "@types/node": "^25.2.3",
    "@types/prompts": "^2.4.9",
    "@types/fs-extra": "^11.0.4"
  },
  "dependencies": {
    "@types/bun": "^1.3.9",
    "vite": "^7.3.1",
    "commander": "^14.0.3",
    "prompts": "^2.4.2",
    "picocolors": "^1.0.0",
    "bundle-require": "^5.0.0",
    "vite-plugin-lumix": "latest",
    "fs-extra": "^11.2.0",
    "vinxi": "^0.5.0",
    "happy-dom": "^20.6.1",
    "@lumix-js/compiler": "latest"
  }
}
